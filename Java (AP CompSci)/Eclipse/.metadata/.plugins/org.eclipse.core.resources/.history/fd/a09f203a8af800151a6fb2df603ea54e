import java.util.ArrayList;


public class Player {

	private ArrayList<Card> Hand;
	
	public Player(ArrayList<Card> hand){
		Hand = hand;
	}
	
	public ArrayList<Card> getCards(){
		return Hand;
	}
	
	public void Saying(){
		for (int i = 0; i < Hand.size(); i++){
			Hand.get(i).saying();
		}
	}
	
	
	
}
